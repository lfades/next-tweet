{
  "name": "next-tweet",
  "version": "0.8.1",
  "repository": "https://github.com/vercel-labs/next-tweet.git",
  "author": "Luis Alvarez (https://twitter.com/luis_fades)",
  "scripts": {
    "build": "pnpm build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "dev": "pnpm build:swc -w",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vercel-labs/next-tweet/issues"
  },
  "sideEffects": [
    "./dist/tweet-container.js"
  ],
  "type": "module",
  "exports": {
    "./theme.css": "./dist/theme.css",
    ".": "./dist/index.js",
    "./*": [
      "./dist/*/index.js",
      "./dist/*.js"
    ]
  },
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*/index",
        "src/*"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "*": [
          "dist/*/index.d.ts",
          "dist/*.d.ts"
        ]
      }
    }
  },
  "peerDependencies": {
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.14",
    "clsx": "^1.2.1",
    "date-fns": "^2.9.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.36",
    "@types/node": "18.13.0",
    "@types/react": "^18.0.27",
    "chokidar": "^3.5.3",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.2.3",
    "typescript": "^4.9.5"
  }
}
